import java.util.*;
class main
{
public static void main(String[] args)
{
Scanner sc=new Scanner(System.in);
int num=sc.nextInt();
int n=num;
while(n>9)
{
int sum=0;
while(n>0)
{
int d=n%10;
n/=10;
sum=sum+d;
}
n=sum
}
if(n==1)
{
System.out.println("MAGIC NUMBER");
}
else
{
system.out.println("NOT A MAGIC NUMBER");
}
}
}
